{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "templates/doinject"
    ],
    "globalMetadata": {
      "_appName": "Doinject",
      "_appTitle": "Doinject",
      "_appLogoPath" : "images/Logo_Header.svg",
      "_enableSearch": false,
      "pdf": false
    }
  }
}